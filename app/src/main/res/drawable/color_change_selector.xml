<?xml version="1.0" encoding="utf-8"?>

<!--选项按下后背景颜色改变-->
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
    	<shape>
    	    <solid android:color="#33333333"/>
		</shape>

    </item>
	<item>
	    <shape>
	        <solid android:color="#ffffff"/>
    	</shape>
	</item>

</selector>